{
  "name": "YouTube Voice search (open source)",
  "version": "0.4",
  "minimum_chrome_version": "49.0",
  "description": "Allow voice search on YouTube web in a similar manner to the phone app.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["content-script.js"]
  }],
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "strict_min_version": "60.0"
    }
  }
}
